{
  "name": "web-tracking-detector",
  "displayName": "Web Tracking Detector",
  "version": "1.0.0",
  "description": "Firefox/Chrome extension that detects connections to third-party domains, injected cookies, HTML local storage and Canvas fingerprinting",
  "author": "MekhyW",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "build:firefox": "plasmo build --target=firefox-mv2",
    "package": "plasmo package",
    "dev:firefox": "plasmo dev --target=firefox-mv2"
  },
  "dependencies": {
    "@types/webextension-polyfill": "^0.10.7",
    "autoprefixer": "^10.4.19",
    "plasmo": "0.86.3",
    "postcss": "^8.4.38",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.33.4",
    "tailwind": "^4.0.0",
    "tailwindcss": "^3.4.3",
    "webextension-polyfill": "^0.12.0",
    "webextension-polyfill-ts": "^0.26.0"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "prettier": "3.2.4",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://*/*"
    ],
    "permissions": [
      "webRequest",
      "webRequestBlocking",
      "cookies",
      "storage",
      "<all_urls>"
    ]
  }
}
